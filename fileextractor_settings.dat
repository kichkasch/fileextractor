# Directory for image file (use leading dot for relative path)
ig_output_dir
./FileExtractorImage.img
# Core for executing the imaging (Win32, Linux oder auto)
ig_default_core
auto
# Location of dd-command (use leading dot for relative path)
ig_location_dd
./dd/dd.exe
# Command to use for getting root privileges - leave blank if you run as root anyway
command_sudo
gksudo --message 'Imaging requires root priveliges - please provide password.'
# Delete image file after processing? (yes / no)
ig_delete_imagefile
yes
# Number of numerical digits in file names of output files
naming_digits
5
# First number in file names for output files
naming_start
1
# Output directory for recovered files (use leading dot for relative path)
output_dir
./
# Disabled Signatures
signatues_off
